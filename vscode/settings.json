{
    "editor.fontSize": 16,
    "editor.scrollBeyondLastLine": true,
    "editor.formatOnType": true,
    "editor.renderWhitespace": "boundary",
    "editor.tabSize": 2,
    "editor.multiCursorModifier": "alt",
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "onFocusChange",
    "window.openFilesInNewWindow": "on",
    "debug.allowBreakpointsEverywhere": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "flow.enabled": true,
    "flow.useNPMPackagedFlow": true,
    "window.zoomLevel": 1,
    "javascript.validate.enable": false,
    "workbench.colorTheme": "Dracula",
    "workbench.iconTheme": "material-icon-theme",
    "editor.minimap.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "editor.accessibilitySupport": "off",
    "eslint.packageManager": "yarn",
    "eslint.options": {
        "rules": {
            // disabling .only auto fixing for editors
            "jest/no-focused-tests": "off",
            // allowing debugger statements
            "no-debugger": "off",
        }
    },
    "javascript.preferences.quoteStyle": "single",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.shell.osx": "zsh",
    "explorer.confirmDelete": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "autoimport.doubleQuotes": false,
    "prettier.requireConfig": true,
    "cSpell.userWords": [
        "Draggable",
        "Reparenting",
        "draggables",
        "droppable",
        "droppables",
        "reparent",
        "scrollable",
        "scrollables",
        "scroller",
        "unmount",
        "unmounts",
        "behaviour"
    ],
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "breadcrumbs.enabled": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.editor.enablePreview": false,
    "liveshare.featureSet": "insiders",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "yaml.schemas": {
        "file:///Users/alex/.vscode/extensions/me-dutour-mathieu.vscode-github-actions-2.2.7/schemas/workflow.schema.json": "/.github/workflows/*.yml",
        "file:///Users/alex/.vscode/extensions/me-dutour-mathieu.vscode-github-actions-2.2.7/schemas/action.schema.json": "/action.yml"
    },
    "cSpell.customUserDictionaries": [

    ],
}
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
}
